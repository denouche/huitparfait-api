{
  "name": "huitparfait-api",
  "version": "0.0.1",
  "description": "huitparfait-api",
  "author": "Huit Parfait Team",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:huitparfait/huitparfait-api.git"
  },
  "engines": {
    "node": "10.2.1"
  },
  "scripts": {
    "dev": "nodemon --watch src --watch config src/start-app.js",
    "start": "node src/start-app.js",
    "e2e": "NODE_ENV=development jest -c ./e2e.jest.config.js",
    "e2e:watch": "NODE_ENV=development jest -c ./e2e.jest.config.js --watch",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "convict": "4.2.0",
    "hapi": "17.5.0",
    "hapi-auth-jwt2": "8.1.0",
    "pg": "7.4.3",
    "sequelize": "4.37.10",
    "sequelize-cli": "4.0.0",
    "sql-tag": "1.0.1"
  },
  "devDependencies": {
    "jest": "23.1.0",
    "jsonwebtoken": "8.2.2",
    "nodemon": "1.17.5",
    "supertest": "3.1.0"
  }
}
